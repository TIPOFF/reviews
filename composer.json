{
    "name": "drewroberts/reporting",
    "description": "Laravel Google API Client",
    "keywords": [
        "reporting"
    ],
    "homepage": "https://github.com/drewroberts/reporting",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Roberts",
            "email": "drew@drewroberts.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DrewRoberts\\Reporting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DrewRoberts\\Reporting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DrewRoberts\\Reporting\\ReportingServiceProvider"
            ],
            "aliases": {
                "Reporting": "DrewRoberts\\Reporting\\ReportingFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
