{
    "name": "drewroberts/laravel-google-api-client",
    "description": "A Laravel package to access Google API's with multiple user credentials",
    "type": "library",
    "keywords": [
        "google",
        "google api",
        "google api client",
        "laravel",
        "laravel wrapper",
        "retrieve",
        "reports"
    ],
    "homepage": "https://github.com/drewroberts/laravel-google-api-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Roberts",
            "email": "github@drewroberts.com",
            "homepage": "https://drewroberts.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "google/apiclient": "^2.0",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "league/flysystem": ">=1.0.8",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DrewRoberts\\Google\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DrewRoberts\\Google\\GoogleServiceProvider"
            ],
            "aliases": {
                "Google": "DrewRoberts\\Google\\Facades\\Google"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DrewRoberts\\Google\\Tests\\": "tests"
        }
    }
}
